<br><br>
<div class="content" style="display:block; text-align:center;">
	<%=image_tag("break_the_bubble.jpg", alt: "Break the Bubble")%><br><br>

        <%= form_tag "/", :class=>"form-search" do %>
          <%= text_field_tag 'search'%>
          <%= select_tag :page, options_for_select((1..10).map {|i| ["Page #{i}", i]}),:style=>"width:90px"%>
          <%= submit_tag "Search", :class=>"btn btn-primary" %>
        <% end %>

</div>
